local Players = game:GetService("Players")
local TeleportService = game:GetService("TeleportService")
local TeleportCheck = false

    queueteleport = (syn and syn.queue_on_teleport) or queue_on_teleport or (fluxus and fluxus.queue_on_teleport)
    httprequest = (syn and syn.request) or (http and http.request) or http_request or (fluxus and fluxus.request) or request

    local PlaceId, JobId = game.PlaceId, game.JobId

    -- Connecting to the OnTeleport event
    Players.LocalPlayer.OnTeleport:Connect(function(State)
        if not TeleportCheck and queueteleport then
            TeleportCheck = true

            -- Queue the PrivateServer join and script execution
            queueteleport([[
                getgenv().performanceMode = true
                loadstring(game:HttpGet('https://raw.githubusercontent.com/Mike0904/MGGS1/refs/heads/main/Ryfie'))()
            ]])
        end
    end)
